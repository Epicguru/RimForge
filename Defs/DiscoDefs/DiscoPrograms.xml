<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <RimForge.DiscoProgramDef Name="RF_BDP" Abstract="true">
    <label>disco program</label>
    <description>If you are reading this I wish you a very pleasant day.</description>
  </RimForge.DiscoProgramDef>


  <!-- Solid color -->
  <RimForge.DiscoProgramDef ParentName="RF_BDP">
    <defName>RF_DP_Solid</defName>
    <programClass>RimForge.Disco.Programs.Solid</programClass>
    <colors>
      <li>(255, 255, 255, 255)</li>
    </colors>
  </RimForge.DiscoProgramDef>


  <!-- Color cycle -->
  <RimForge.DiscoProgramDef Name="RF_DP_BaseCycle" ParentName="RF_BDP" Abstract="true">
    <programClass>RimForge.Disco.Programs.ColorCycle</programClass>
    <inputs>
      <transitionTime>80</transitionTime>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_BaseCycle">
    <defName>RF_DP_Cycle_Vapourwave</defName>
    <inputs>
      <colors>
          (255,113,206):
          (1,205,254):
          (5,255,161):
          (185,103,255):
          (255,251,150)
        </colors>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_BaseCycle">
    <defName>RF_DP_Cycle_Sunrise</defName>
    <inputs>
      <colors>
          (253,191,96):
          (240,125,96):
          (210,68,154):
          (120,52,124):
          (58,21,96)
        </colors>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_BaseCycle">
    <defName>RF_DP_Cycle_Soul</defName>
    <inputs>
      <colors>
          (198,50,84):
          (236,100,73):
          (249,153,75):
          (251,203,90):
          (42,162,163)
        </colors>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_BaseCycle">
    <defName>RF_DP_Cycle_Radioactive</defName>
    <inputs>
      <colors>
          (181,255,0):
          (117,255,0):
          (0,255,38):
          (0,255,153):
          (0,255,205)
        </colors>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_BaseCycle">
    <defName>RF_DP_Cycle_Pastel</defName>
    <inputs>
      <colors>
          (255,179,186):
          (255,223,186):
          (255,255,186):
          (186,255,201):
          (186,225,255)
        </colors>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_BaseCycle">
    <defName>RF_DP_Cycle_Neon</defName>
    <inputs>
      <colors>
          (254,0,0):
          (253,254,2):
          (11,255,1):
          (1,30,254):
          (254,0,246)
        </colors>
    </inputs>
  </RimForge.DiscoProgramDef>


  <!-- Checkerboards -->
  <RimForge.DiscoProgramDef Name="RF_DP_CheckerboardBase" ParentName="RF_BDP" Abstract="true">
    <programClass>RimForge.Disco.Programs.Checkerboard</programClass>
    <inputs>
      <swapInterval>20</swapInterval>
      <swapTime>10</swapTime>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_CheckerboardBase">
    <defName>RF_DP_Checkerboard_PurpleYellow</defName>
    <inputs>
      <colorA>(197, 89, 227)</colorA>
      <colorB>(255, 248, 166)</colorB>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_CheckerboardBase">
    <defName>RF_DP_Checkerboard_CyanBlue</defName>
    <inputs>
      <colorA>(36, 255, 248)</colorA>
      <colorB>(87, 93, 181)</colorB>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_CheckerboardBase">
    <defName>RF_DP_Checkerboard_Spain</defName>
    <inputs>
      <colorA>(189, 9, 9)</colorA>
      <colorB>(232, 207, 16)</colorB>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_CheckerboardBase">
    <defName>RF_DP_Checkerboard_Grayscale</defName>
    <inputs>
      <colorA>(190, 190, 190)</colorA>
      <colorB>(255, 255, 255)</colorB>
    </inputs>
  </RimForge.DiscoProgramDef>


  <!-- Noise -->
  <RimForge.DiscoProgramDef Name="RF_DP_NoiseBase" ParentName="RF_BDP" Abstract="true">
    <programClass>RimForge.Disco.Programs.Noise</programClass>
    <inputs>
      <scale>2</scale>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_NoiseBase">
    <defName>RF_DP_Noise_Default</defName>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_NoiseBase">
    <defName>RF_DP_Noise_Light</defName>
    <inputs>
      <add>0.2</add>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_NoiseBase">
    <defName>RF_DP_Noise_VeryLight</defName>
    <inputs>
      <add>0.55</add>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_NoiseBase">
    <defName>RF_DP_Noise_Smooth</defName>
    <inputs>
      <scale>0.15</scale>
      <add>0.2</add>
    </inputs>
  </RimForge.DiscoProgramDef>


  <!-- Edge distance -->
  <RimForge.DiscoProgramDef Name="RF_DP_EdgeBase" ParentName="RF_BDP" Abstract="true">
    <programClass>RimForge.Disco.Programs.EdgeDistance</programClass>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_EdgeBase">
    <defName>RF_DP_EdgeDefault</defName>
  </RimForge.DiscoProgramDef>

  <!-- Ripples -->
  <RimForge.DiscoProgramDef Name="RF_DP_RippleBase" ParentName="RF_BDP" Abstract="true">
    <programClass>RimForge.Disco.Programs.Ripple</programClass>
    <colors>
      <li>(1, 1, 1, 0)</li> <!-- 'low' color -->
      <li>(1, 1, 1, 1)</li> <!-- 'high' color -->
    </colors>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef Name="RF_DP_Ripple_BW_Out" ParentName="RF_DP_RippleBase">
    <defName>RF_DP_Ripple_BW_Out</defName>
    <bools>
      <li>true</li> <!-- Circular? -->
    </bools>
    <floats>
      <li>-2</li> <!-- Initial radius -->
      <li>2</li> <!-- Thickness -->
      <li>0.1</li> <!-- Radius change per tick -->
      <li>22</li> <!-- Despawn after radius reaches this -->
    </floats>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef Name="RF_DP_Ripple_BW_In" ParentName="RF_DP_RippleBase">
    <defName>RF_DP_Ripple_BW_In</defName>
    <bools>
      <li>true</li>
    </bools>
    <floats>
      <li>22</li>
      <li>2</li>
      <li>-0.1</li>
      <li>-2</li>
    </floats>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_Ripple_BW_Out">
    <defName>RF_DP_Ripple_BW_Out_Fast</defName>
    <floats Inherit="false">
      <li>3.5</li>
      <li>1</li>
      <li>0.3</li>
      <li>22</li>
    </floats>
  </RimForge.DiscoProgramDef>

  <!-- Flying lines -->
  <RimForge.DiscoProgramDef Name="RF_DP_LineBase" ParentName="RF_BDP" Abstract="true">
    <programClass>RimForge.Disco.Programs.FlyingLine</programClass>
    <inputs>
      <lineColor>(1, 1, 1, 1)</lineColor>
      <defaultColor>(0, 0, 0, 0)</defaultColor>
      <moveInterval>4</moveInterval>
      <forwards>true</forwards>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef Name="RF_DP_Line_BW_Hor" ParentName="RF_DP_LineBase">
    <defName>RF_DP_Line_BW_Hor</defName>
    <inputs>
      <direction>0</direction>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_Line_BW_Hor">
    <defName>RF_DP_Line_BW_Hor_Back</defName>
    <inputs>
      <forwards>false</forwards>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef Name="RF_DP_Line_BW_Vert" ParentName="RF_DP_LineBase">
    <defName>RF_DP_Line_BW_Vert</defName>
    <inputs>
      <direction>1</direction>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_Line_BW_Vert">
    <defName>RF_DP_Line_BW_Vert_Back</defName>
    <inputs>
      <forwards>false</forwards>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef Name="RF_DP_Line_BW_Diag" ParentName="RF_DP_LineBase">
    <defName>RF_DP_Line_BW_Diag</defName>
    <inputs>
      <direction>2</direction>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_Line_BW_Diag">
    <defName>RF_DP_Line_BW_Diag_Back</defName>
    <inputs>
      <forwards>false</forwards>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef Name="RF_DP_Line_BW_Diag_Inv" ParentName="RF_DP_LineBase">
    <defName>RF_DP_Line_BW_Diag_Inv</defName>
    <inputs>
      <direction>3</direction>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_Line_BW_Diag_Inv">
    <defName>RF_DP_Line_BW_Diag_Inv_Back</defName> <!-- Def names be like: RimForge disco program line black-and-white diagonal inverted backwards -->
    <inputs>
      <forwards>false</forwards>
    </inputs>
  </RimForge.DiscoProgramDef>


  <!-- Fade In/Out -->
  <RimForge.DiscoProgramDef ParentName="RF_BDP">
    <defName>RF_DP_FadeIn</defName>
    <programClass>RimForge.Disco.Programs.Fade</programClass>
    <inputs>
      <duration>30</duration>
      <fadeIn>true</fadeIn>
    </inputs>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_BDP">
    <defName>RF_DP_FadeOut</defName>
    <programClass>RimForge.Disco.Programs.Fade</programClass>
    <inputs>
      <duration>30</duration>
      <fadeIn>false</fadeIn>
    </inputs>
  </RimForge.DiscoProgramDef>


  <!-- Stripes -->
  <RimForge.DiscoProgramDef Name="RF_DP_StripesBase" ParentName="RF_BDP" Abstract="true">
    <programClass>RimForge.Disco.Programs.Stripes</programClass>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef Name="RF_DP_Stripes_Hor" ParentName="RF_DP_StripesBase" Abstract="true">
    <bools>
      <li>true</li> <!-- Horizontal? -->
    </bools>
    <ints>
      <li>2</li> <!-- Every Nth tile -->
      <li>20</li> <!-- Shift interval -->
      <li>1</li> <!-- Shift direction -->
    </ints>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_Stripes_Hor">
    <defName>RF_DP_Stripes_Hor_Greyscale</defName>
    <colors>
      <li>(255, 255, 255)</li>
      <li>(200, 200, 200)</li>
      <li>(120, 120, 120)</li>
      <li>(60, 60, 60)</li>
    </colors>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef Name="RF_DP_Stripes_Vert" ParentName="RF_DP_StripesBase" Abstract="true">
    <bools>
      <li>false</li>
    </bools>
    <ints>
      <li>2</li>
      <li>20</li>
      <li>1</li>
    </ints>
  </RimForge.DiscoProgramDef>

  <RimForge.DiscoProgramDef ParentName="RF_DP_Stripes_Vert">
    <defName>RF_DP_Stripes_Vert_Greyscale</defName>
